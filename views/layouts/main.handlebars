<body>
  <div id="background"></div>

  <nav class="navbar navbar-expand-lg navbar-light bg-light fixed-top">
    <a class="navbar-brand" href="/">PROPERTY.HK</a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent"
      aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>

    <div class="collapse navbar-collapse" id="navbarSupportedContent">
      <div class="my-2 my-lg-0">
        <input id="locationName" class="form-control mr-sm-2" type="text" placeholder="Enter an address..." aria-label="Search">
        <div class="btn-group">


          <button id="letssearch" class="btn btn-secondary my-2 my-sm-0">GO</button>
          <button class="btn btn-secondary my-2 my-sm-0" data-toggle="collapse" data-target="#collapseExample" onclick="window.scrollTo(0, 0);">Advance
            Setting</button>
        </div>

      </div>
      <ul class="navbar-nav ml-auto">
        <li class="nav-item active">
          <a class="nav-link" href="/">Home <span class="sr-only">(current)</span></a>
        </li>
        {{#if user}}
        <li class="nav-item">
          <a class="nav-link" href="/user">ID: {{user.username}}</a>
        </li>
        <li class="nav-item">
          <a class="nav-link" href="/logout">Logout</a>
        </li>
        {{else}}
        <li class="nav-item">
          <a class="nav-link" id="register" href="">Register</a>
        </li>
        <li class="nav-item">
          <a class="nav-link" id="login" href="">Login</a>
        </li>
        {{/if}}
      </ul>
    </div>
  </nav>
  {{#if user}}
  <div class="collapse card-body col-12 collapsey" id="collapseExample">
    <br />
    <div class="card card-body">
      <form action="/" method="POST">
        <div class="regions row">
          <div class="bRegion col-3" id="HK">
            <label for="HK"><strong>HK</strong></label><br>
          </div>
          <div class="bRegion col-3" id="KLN">
            <label for="KLN"><strong>KLN</strong></label><br>
          </div>
          <div class="bRegion col-3" id="NTE">
            <label for="NTE"><strong>NTE</strong></label><br>
          </div>
          <div class="bRegion col-3" id="NTW">
            <label for="NTW"><strong>NTW</strong></label><br>
          </div>
        </div>

        <div class="others row">
          <div class="actualArea col-2">
            <p><strong>Actual Area</strong></p><br>
            <label class="control control-radio">
              below 300 ft
              <input type="radio" name="actualArea" value="<300">
              <div class="control_indicator"></div>
            </label>
            <label class="control control-radio">
              300-600 ft
              <input type="radio" name="actualArea" value="300-600">
              <div class="control_indicator"></div>
            </label>
            <label class="control control-radio">
              600-1000 ft
              <input type="radio" name="actualArea" value="600-1000">
              <div class="control_indicator"></div>
            </label>
            <label class="control control-radio">
              above 1000 ft
              <input type="radio" name="actualArea" value=">1000">
              <div class="control_indicator"></div>
            </label>
          </div>
          <div class="price col-2">
            <p><strong>Price</strong></p><br>
            <label class="control control-radio">
              <input type="radio" name="price" value="<3">below 3M<br>
              <div class="control_indicator"></div>
            </label>
            <label class="control control-radio">
              <input type="radio" name="price" value="3-6">3-6M<br>
              <div class="control_indicator"></div>
            </label>
            <label class="control control-radio">
              <input type="radio" name="price" value="6-10">6-10M<br>
              <div class="control_indicator"></div>
            </label>
            <label class="control control-radio">
              <input type="radio" name="price" value=">10">above 10M<br>
              <div class="control_indicator"></div>
            </label>
          </div>
          <div class="actualPrice col-2">
            <p><strong>Actual Price</strong></p><br>
            <label class="control control-radio">
              <input type="radio" name="actualPrice" value="<10000">below 10000<br>
              <div class="control_indicator"></div>
            </label>
            <label class="control control-radio">
              <input type="radio" name="actualPrice" value="10000-15000">10000-15000<br>
              <div class="control_indicator"></div>
            </label>
            <label class="control control-radio">
              <input type="radio" name="actualPrice" value="15000-20000">15000-20000<br>
              <div class="control_indicator"></div>
            </label>
            <label class="control control-radio radio">
              <input type="radio" name="actualPrice" value=">20000">above 20000<br>
              <div class="control_indicator"></div>
            </label>
          </div>
          <div class="date col-2">
            <p><strong>Time Range</strong></p><br>
            <label class="control control-radio">
              <input type="radio" name="date" value="30">Last 30 days<br>
              <div class="control_indicator"></div>
            </label>
            <label class="control control-radio">
              <input type="radio" name="date" value="180">Last 180 days<br>
              <div class="control_indicator"></div>
            </label>
            <label class="control control-radio">
              <input  type="radio" name="date" value="365">Whole year
              <div class="control_indicator"></div>
            </label>
          </div>
          <div class="charts col-2">
            <p><strong>Chart type</strong></p><br>
            <label class="control control-radio">
                        <input type="radio" name="chart" value="histogram">Histogram<br>
              <div class="control_indicator"></div>
            </label>
            <label class="control control-radio">
            <input  type="radio" name="chart" value="candle">Candle<br>
                          <div class="control_indicator"></div>
            </label>
          </div>
        </div>

        <div class="button filterbutt">
          <button id="good" class="btn btn-primary" data-toggle="collapse" href="#collapseExample">Submit</button>
        </div>
      </form>
    </div>
  </div>
