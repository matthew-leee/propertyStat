
	<div class="user container" id="fuck">
		<div class="row">
			<div class="col-3 sidebar">
				<div><a href="/logout">Log Out</a></div>
				<div><a href="/calculator">Mortgage Calculator</a></div>
				<div><a href="/updateUser">Update User Info</a></div>


			</div>

			<div class="col-9 mainbar">
				<h2>User Profile</h2>

				<p>Name: {{user.username}}</p>

				<p>Email: {{user.email}}</p>

				<p>Phone: {{user.phone}}</p>

				<p>Gender: {{user.gender}}</p>

				<p>Salary: {{user.salary}}</p>

				<p>Current District: {{user.district_current}}</p>

				<h2>Bookmarked Property</h2>
				<ul>
					{{#each bookmark}}
						<li>{{this}}</li>
					{{/each}}
				</ul>
			</div>
		</div>	
	</div>
	
